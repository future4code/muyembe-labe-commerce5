arAotopo\"><Subir src={subir}/></a>\r\n\r\n          <Rodape id=\"astro\">\r\n            <h3>Labecommerce faz parte da corporação Astrodev<Astrodev src={astrodev}/></h3>\r\n            <p></p>\r\n          </Rodape>\r\n\r\n        </DivisaoPaginaPrincipal>\r\n\r\n        <DiviCarrinhoDeCompras>\r\n          { componenteCarrinhoDeCompras }\r\n        </DiviCarrinhoDeCompras>\r\n        \r\n      </Principal>\r\n  \r\n    ) \r\n  }\r\n}\r\n\r\nexport default App;",{"ruleId":"35","severity":1,"message":"36","line":6,"column":10,"nodeType":"37","messageId":"38","endLine":6,"endColumn":15},{"ruleId":"35","severity":1,"message":"39","line":6,"column":17,"nodeType":"37","messageId":"38","endLine":6,"endColumn":23},{"ruleId":"35","severity":1,"message":"40","line":6,"column":25,"nodeType":"37","messageId":"38","endLine":6,"endColumn":31},{"ruleId":"35","severity":1,"message":"41","line":7,"column":8,"nodeType":"37","messageId":"38","endLine":7,"endColumn":25},{"ruleId":"35","severity":1,"message":"42","line":76,"column":7,"nodeType":"37","messageId":"38","endLine":76,"endColumn":25},{"ruleId":"43","severity":1,"message":"44","line":349,"column":66,"nodeType":"45","messageId":"46","endLine":349,"endColumn":68},{"ruleId":"43","severity":1,"message":"44","line":369,"column":61,"nodeType":"45","messageId":"46","endLine":369,"endColumn":63},{"ruleId":"43","severity":1,"message":"47","line":457,"column":59,"nodeType":"45","messageId":"46","endLine":457,"endColumn":61},{"ruleId":"43","severity":1,"message":"47","line":476,"column":59,"nodeType":"45","messageId":"46","endLine":476,"endColumn":61},{"ruleId":"48","severity":1,"message":"49","line":533,"column":7,"nodeType":"50","messageId":"51","endLine":551,"endColumn":8},{"ruleId":"43","severity":1,"message":"44","line":538,"column":75,"nodeType":"45","messageId":"46","endLine":538,"endColumn":77},{"ruleId":"43","severity":1,"message":"44","line":545,"column":75,"nodeType":"45","messageId":"46","endLine":545,"endColumn":77},"no-un